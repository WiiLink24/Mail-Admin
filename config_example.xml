<Config>
    <!-- The address which HTTP will bind to -->
    <address>127.0.0.1:9011</address>

    <!-- Credentials -->
    <username>username</username>
    <password>password</password>

    <wiiMailUsername>username</wiiMailUsername>
    <wiiMailPassword>password</wiiMailPassword>

    <!-- Database information-->
    <databaseAddress>127.0.0.1</databaseAddress>
    <databaseName>wiilink_accounts</databaseName>

    <wiiMailDatabaseAddress>127.0.0.1</wiiMailDatabaseAddress>
    <wiiMailDatabaseName>wiilink_accounts</wiiMailDatabaseName>

    <!-- OIDC credentials/data -->
    <oidc>
        <clientID></clientID>
        <clientSecret></clientSecret>
        <redirectURL>http://localhost:9011/authorize</redirectURL>
        <scopes>openid profile email</scopes>
        <provider></provider>
    </oidc>
</Config>