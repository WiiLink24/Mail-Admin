<Config>
    <!-- The address which HTTP will bind to -->
    <address>127.0.0.1:9011</address>
    <assetsPath>assets</assetsPath>

    <!-- OIDC credentials/data -->
    <oidc>
        <clientID></clientID>
        <clientSecret></clientSecret>
        <redirectURL>http://localhost:9011/authorize</redirectURL>
        <scopes>openid profile email</scopes>
        <provider></provider>
    </oidc>
</Config>